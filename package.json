{
  "name": "cute-wallpapers-extension",
  "version": "1.0.0",
  "description": "A high-performance browser extension that transforms your new tab page into a delightful experience with cute wallpapers from Pexels and Unsplash, featuring advanced caching for instant loading and offline support.",
  "main": "newtab.js",
  "scripts": {
    "lint": "eslint *.js",
    "format": "prettier --write *.js *.html *.css",
    "validate": "web-ext lint",
    "build": "web-ext build",
    "start": "web-ext run --target=chromium",
    "test": "echo \"Tests coming soon\" && exit 0",
    "zip": "zip -r cute-wallpapers-extension.zip . -x '*.git*' '*.env*' 'node_modules/*' '*.zip' '*.md' 'docs/*'"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "brave-extension",
    "wallpapers",
    "new-tab",
    "pexels",
    "unsplash",
    "cute",
    "cache",
    "indexeddb",
    "performance"
  ],
  "author": "Francis Mule",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francismul/cute_wallpaper_chromium_browser_extension.git"
  },
  "bugs": {
    "url": "https://github.com/francismul/cute_wallpaper_chromium_browser_extension/issues"
  },
  "homepage": "https://github.com/francismul/cute_wallpaper_chromium_browser_extension#readme",
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "web-ext": "^7.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions"
  ],
  "manifest": {
    "manifest_version": 3,
    "minimum_chrome_version": "88"
  }
}